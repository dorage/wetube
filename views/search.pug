extends layouts/main
include mixins/videoBlock

block content
    .search_header
        h3 Searching by #{searchingBy}
    .search_videos
        if videos.length == 0
            h5 No Videos file
        each video in videos
            +videoBlock({
                title: video.title,
                views: video.views,
                videoFile: video.videoFile,
                id: video.id
            })